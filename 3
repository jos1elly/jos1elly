programa {
  funcao inicio() {
    inteiro n1, n2, n3, r
    caracter notas

    escreva ("digite a primeira nota:\n")
    leia (n1)
    escreva ("digite a segunda nota:\n")
    leia (n2)
    escreva ("digite a terceira nota:\n")
    leia (n3)

    r= n1 + n2 + n3 /3
    escreva("A sua média é:\n", r)
  }
}
